{
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[red]": {},
  "ansible.lightspeed.enabled": false,
  "bitbucket-pull-requests.serverURL": "https://bosenbbucket1.netscout.com:8443",
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.userWords": [
    "benbow",
    "direnv",
    "EMDB",
    "envrc",
    "isort",
    "jgaines",
    "Justfile",
    "levelname",
    "localzone",
    "netscout",
    "pipx",
    "pyproject",
    "revset",
    "Securden",
    "tzlocal",
    "virtualenv",
    "werkzeug"
  ],
  "editor.fontFamily": "FiraCode Nerd Font Mono, 'monospace', monospace",
  "editor.fontVariations": false,
  "editor.overviewRulerBorder": false,
  "editor.rulers": [
    80,
    120
  ],
  "errorLens.excludeBySource": [
    "cSpell"
  ],
  "errorLens.messageEnabled": false,
  "files.autoSave": "onFocusChange",
  "git.blame.editorDecoration.enabled": false,
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",
  "git.suggestSmartCommit": false,
  "gitblame.revsFile": [
    ".git-blame-ignore-revs"
  ],
  "github.copilot.chat.codeGeneration.instructions": [
    {
      "file": "${workspaceFolder}/.github/copilot-instructions.md"
    }
  ],
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.testGeneration.instructions": [
    {
      "file": "${workspaceFolder}/.github/copilot-instructions.md"
    }
  ],
  "github.copilot.enable": {
    "markdown": true
  },
  "github.copilot.nextEditSuggestions.fixes": true,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "indentRainbow.errorColor": "rgba(128,32,32,0.2)",
  "markdownlint.config": {
    "MD034": false
  },
  "mise.binPath": "/home/jgaines/.local/bin/mise",
  "projectManager.git.baseFolders": [
    "~/git/eng-tools",
    "~/git/administrator",
    "~/projects"
  ],
  "projectManager.tags": [
    "Personal",
    "Work",
    "POC",
    "WIP"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv",
  "red.redDir": "$HOME/apps/red",
  "redhat.telemetry.enabled": false,
  "roc-lang.language-server.exe": "/home/jgaines/.loca/bin/roc_language_server",
  "visualjj.showSourceControlColocated": true,
  "workbench.colorTheme": "Default Light Modern",
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.ipynb": "jupyter-notebook",
    "*.md": "default"
  },
  "zig.path": "zig",
  "zig.zls.enabled": "on",
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.scrollback": 10000,
  "editor.renderWhitespace": "boundary",
  "docker.extension.enableComposeLanguageServer": false,
  "git.enabled": false,
  "gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true,
    "suppressGitMissingWarning": true
  }
}
